  "author": "Your Name",
  "license": "MIT",
  "type": "module",
  "private": false,
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5"
  },
  "peerDependencies": {
    "@aws-sdk/client-rds-data": "^3.0.0",
    "drizzle-orm": "^0.33.0",
    "pg": "^8.0.0",
    "typescript": "^5"
  },
  "peerDependenciesMeta": {
    "pg": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
