{
  "name": "@whoof/database",
  "version": "1.0.0",
  "description": "Database utilities and connection management for Whoof applications using AWS RDS Data API and Drizzle ORM",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "types": "./src/index.ts"
    }
