    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AlbertoV5/whoof-database.git",
    "directory": "packages/lib-database"
  },
  "keywords": [
    "database",
    "aws",
    "rds",
    "drizzle",
    "postgresql",
    "whoof",
    "typescript"
